{
    "version": 1,
    "cli": {
        "defaultCollection": "@nrwl/angular",
        "analytics": false
    },
    "schematics": {
        "@nrwl/angular": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            },
            "storybook-configuration": {
                "linter": "eslint"
            }
        },
        "@nrwl/angular:application": {
            "style": "scss",
            "linter": "eslint",
            "unitTestRunner": "jest",
            "e2eTestRunner": "cypress"
        },
        "@nrwl/angular:library": {
            "style": "scss",
            "linter": "eslint",
            "unitTestRunner": "jest"
        },
        "@nrwl/angular:component": {
            "style": "scss"
        }
    },
    "defaultProject": "back-office",
    "projects": {
        "back-office": {
            "projectType": "application",
            "root": "apps/back-office",
            "sourceRoot": "apps/back-office/src",
            "prefix": "nicecactus-platform",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/apps/back-office",
                        "index": "apps/back-office/src/index.html",
                        "main": "apps/back-office/src/main.ts",
                        "polyfills": "apps/back-office/src/polyfills.ts",
                        "tsConfig": "apps/back-office/tsconfig.app.json",
                        "aot": true,
                        "assets": ["apps/back-office/src/favicon.ico", "apps/back-office/src/assets"],
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
                            "apps/back-office/src/styles.scss"
                        ],
                        "scripts": [
                            "apps/front/src/assets/vendors/jquery/dist/jquery.min.js",
                            "apps/front/src/assets/vendors/bootstrap/dist/js/bootstrap.bundle.min.js",
                            "apps/front/src/assets/vendors/js-cookie/js.cookie.js",
                            "apps/front/src/assets/vendors/jquery.scrollbar/jquery.scrollbar.min.js",
                            "apps/front/src/assets/js/argon.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/back-office/src/environments/environment.ts",
                                    "with": "apps/back-office/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "back-office:build",
                        "proxyConfig": "apps/front/proxy.conf.json"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "back-office:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "back-office:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/back-office/src/**/*.ts", "apps/back-office/src/**/*.html"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/back-office"],
                    "options": {
                        "jestConfig": "apps/back-office/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "back-office-e2e": {
            "root": "apps/back-office-e2e",
            "sourceRoot": "apps/back-office-e2e/src",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/back-office-e2e/cypress.json",
                        "tsConfig": "apps/back-office-e2e/tsconfig.e2e.json",
                        "devServerTarget": "back-office:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "back-office:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/back-office-e2e/**/*.{js,ts}"]
                    }
                }
            }
        },
        "front": {
            "projectType": "application",
            "root": "apps/front",
            "sourceRoot": "apps/front/src",
            "prefix": "nicecactus-platform",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/apps/front",
                        "index": "apps/front/src/index.html",
                        "main": "apps/front/src/main.ts",
                        "polyfills": "apps/front/src/polyfills.ts",
                        "tsConfig": "apps/front/tsconfig.app.json",
                        "aot": true,
                        "assets": ["apps/front/src/favicon.ico", "apps/front/src/assets"],
                        "styles": [
                            "apps/front/src/styles.scss",
                            "node_modules/font-awesome/css/font-awesome.css",
                            "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css"
                        ],
                        "scripts": [
                            "apps/front/src/assets/vendors/jquery/dist/jquery.min.js",
                            "apps/front/src/assets/vendors/bootstrap/dist/js/bootstrap.bundle.min.js",
                            "apps/front/src/assets/vendors/js-cookie/js.cookie.js",
                            "apps/front/src/assets/vendors/jquery.scrollbar/jquery.scrollbar.min.js",
                            "apps/front/src/assets/js/argon.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/front/src/environments/environment.ts",
                                    "with": "apps/front/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "front:build",
                        "proxyConfig": "apps/front/proxy.conf.json"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "front:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "front:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/front/src/**/*.ts", "apps/front/src/**/*.html"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/front"],
                    "options": {
                        "jestConfig": "apps/front/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "front-e2e": {
            "root": "apps/front-e2e",
            "sourceRoot": "apps/front-e2e/src",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/front-e2e/cypress.json",
                        "tsConfig": "apps/front-e2e/tsconfig.e2e.json",
                        "devServerTarget": "front:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "front:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/front-e2e/**/*.{js,ts}"]
                    }
                }
            }
        },
        "api": {
            "root": "apps/api",
            "sourceRoot": "apps/api/src",
            "projectType": "application",
            "prefix": "api",
            "architect": {
                "build": {
                    "builder": "@nrwl/node:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/api",
                        "main": "apps/api/src/main.ts",
                        "tsConfig": "apps/api/tsconfig.app.json",
                        "assets": ["apps/api/src/assets"]
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false,
                            "fileReplacements": [
                                {
                                    "replace": "apps/api/src/environments/environment.ts",
                                    "with": "apps/api/src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/node:execute",
                    "options": {
                        "buildTarget": "api:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/api/**/*.ts"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/api"],
                    "options": {
                        "jestConfig": "apps/api/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "types": {
            "projectType": "library",
            "root": "libs/types",
            "sourceRoot": "libs/types/src",
            "prefix": "nicecactus-platform",
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/types/src/**/*.ts", "libs/types/src/**/*.html"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/types"],
                    "options": {
                        "jestConfig": "libs/types/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "form-validators": {
            "projectType": "library",
            "root": "libs/form-validators",
            "sourceRoot": "libs/form-validators/src",
            "prefix": "nicecactus-platform",
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/form-validators/src/**/*.ts", "libs/form-validators/src/**/*.html"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/form-validators"],
                    "options": {
                        "jestConfig": "libs/form-validators/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "pipes": {
            "projectType": "library",
            "root": "libs/pipes",
            "sourceRoot": "libs/pipes/src",
            "prefix": "nicecactus-platform",
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/pipes/src/**/*.ts", "libs/pipes/src/**/*.html"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/pipes"],
                    "options": {
                        "jestConfig": "libs/pipes/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "graphql-layer": {
            "projectType": "library",
            "root": "libs/graphql-layer",
            "sourceRoot": "libs/graphql-layer/src",
            "prefix": "nicecactus-platform",
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/graphql-layer/src/**/*.ts", "libs/graphql-layer/src/**/*.html"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/graphql-layer"],
                    "options": {
                        "jestConfig": "libs/graphql-layer/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "graph-ql-service": {
            "projectType": "library",
            "root": "libs/graph-ql-service",
            "sourceRoot": "libs/graph-ql-service/src",
            "prefix": "nicecactus-platform",
            "architect": {
                "build": {
                    "builder": "@nrwl/angular:ng-packagr-lite",
                    "options": {
                        "tsConfig": "libs/graph-ql-service/tsconfig.lib.json",
                        "project": "libs/graph-ql-service/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "libs/graph-ql-service/tsconfig.lib.prod.json"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/graph-ql-service/src/**/*.ts", "libs/graph-ql-service/src/**/*.html"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/graph-ql-service"],
                    "options": {
                        "jestConfig": "libs/graph-ql-service/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        }
    }
}
