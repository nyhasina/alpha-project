// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Contact {
  id    Int     @default(autoincrement()) @id
  forename  String?
  surname  String?
}

model User {
  id  Int @default(autoincrement()) @id
  email String
  password String
}
