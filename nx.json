{
  "npmScope": "nicecactus-platform",
  "affected": { "defaultBase": "master" },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "back-office": { "tags": [] },
    "back-office-e2e": { "tags": [], "implicitDependencies": ["back-office"] },
    "front": { "tags": [] },
    "front-e2e": { "tags": [], "implicitDependencies": ["front"] }
  }
}
